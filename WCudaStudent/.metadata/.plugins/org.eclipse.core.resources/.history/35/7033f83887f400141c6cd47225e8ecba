#include <iostream>
#include <limits.h>
#include "saucissonSMHost.h"

using std::cout;
using std::endl;

bool useSaucisson(void)
    {
    cout << endl << "[SaucissonSM]" << endl;

    bool isOk = true;
    saucissonSMHost saucisson = saucissonSMHost(128);
    saucisson.process();

    isOk&= saucisson.isOk();

    return isOk;
    }

